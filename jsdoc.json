{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.tsx?$",
      "excludePattern": "(^|\\/|\\\\)node_modules"
    },
    "plugins": ["node_modules/jsdoc-babel"],
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "node_modules/minami"
    }
}